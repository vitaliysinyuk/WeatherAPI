{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "NASAPower": {
    "Daily": {
      "nasaPowerBaseURL": "https://power.larc.nasa.gov/api/temporal/daily/point?",
      "community": "community=AG",
      "parameters": "parameters=T2M,T2M_MAX,T2M_MIN,PRECTOTCORR",
      "timeStandard": "units=imperial",
      "format": "format=json"
    }
  }
}
